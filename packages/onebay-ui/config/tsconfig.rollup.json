{
  "compilerOptions": {
    "skipLibCheck": true,
    "target": "es5",
    "jsx": "preserve",
    "jsxFactory": "h",
    "module": "es6",
    "downlevelIteration": true,
    "baseUrl": "../",
    "types": ["node"],
    "emitDeclarationOnly": false,
    "declarationDir": "../dist",
    "noEmit": true
  },
  "include": [
    "../src"
  ],
  "exclude": [
    "../node_modules/*",
    "../src/**/*.spec.tsx"
  ]
}
